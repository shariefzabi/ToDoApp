 1+null=2
 1+undefined =NAN
 undefined+null =NAN
 new Date()+2='2Sat Dec 02 2023 12:47:44 GMT+0530 (India Standard Time)'
 a=[]
 a+1 ='1'
 b={"hii":"hello"}
 b+2='[object Object]2'
